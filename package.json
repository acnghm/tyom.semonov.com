{
  "name": "tyom-semonov.cv",
  "description": "Tyom Semonov: CV",
  "private": true,
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "start": "node __sapper__/build",
    "export": "sapper build && ./bin/create-pdf && sapper export --legacy",
    "serve": "sirv __sapper__/export",
    "serve-dist": "sirv dist",
    "storybook": "start-storybook -p 6006 --ci --static-dir ./static",
    "build-storybook": "build-storybook -o dist/storybook --static-dir ./static",
    "prepare-deploy": "rm -rf dist/ && yarn export && yarn build-storybook && cp -r __sapper__/export/* dist/",
    "deploy": "gh-pages -d dist --dotfiles"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "is-plain-object": "^3.0.0",
    "js-yaml": "3.13.1",
    "markdown-it": "10.0.0",
    "polka": "^1.0.0-next.11",
    "popper.js": "1.16.0",
    "sirv": "^1.0.0-next.2",
    "svelte-icons": "2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/runtime": "^7.8.7",
    "@codechecks/build-size-watcher": "^0.1.0",
    "@codechecks/client": "^0.1.10",
    "@codechecks/lighthouse-keeper": "^0.1.2",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/svelte": "^5.3.14",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-tyom": "^2.6.0",
    "gh-pages": "2.2.0",
    "postcss": "^7.0.27",
    "postcss-import": "12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "puppeteer": "2.1.1",
    "raw-loader": "^4.0.0",
    "rollup": "^2.0.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
