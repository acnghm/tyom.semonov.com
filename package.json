{
  "name": "tyom-semonov.cv",
  "description": "Tyom Semonov: CV",
  "private": true,
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "start": "node __sapper__/build",
    "export": "sapper build && ./bin/create-pdf && sapper export --legacy",
    "serve": "sirv __sapper__/export",
    "serve-dist": "sirv dist",
    "storybook": "start-storybook -p 6006 --ci --static-dir ./static",
    "build-storybook": "build-storybook -o dist/storybook --static-dir ./static",
    "prepare-deploy": "rm -rf dist/ && yarn export && yarn build-storybook && cp -r __sapper__/export/* dist/",
    "deploy": "gh-pages -d dist --dotfiles"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "is-plain-object": "^3.0.0",
    "js-yaml": "3.13.1",
    "markdown-it": "10.0.0",
    "polka": "next",
    "popper.js": "1.15.0",
    "sirv": "^0.4.2",
    "svelte-icons": "1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@storybook/addon-actions": "^5.2.0",
    "@storybook/addon-links": "^5.2.0",
    "@storybook/addons": "^5.2.0",
    "@storybook/svelte": "^5.2.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "gh-pages": "2.1.1",
    "postcss": "^7.0.18",
    "postcss-import": "12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "prettier-plugin-svelte": "^0.7.0",
    "puppeteer": "1.20.0",
    "raw-loader": "^3.1.0",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "rucksack-css": "1.0.2",
    "sapper": "^0.27.8",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.12.1",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.1.1"
  }
